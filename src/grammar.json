{
  "name": "ConTeXt",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_content"
      }
    },
    "_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "escaped"
        },
        {
          "type": "SYMBOL",
          "name": "group"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "SYMBOL",
          "name": "command"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_opcontent": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "escaped"
        },
        {
          "type": "SYMBOL",
          "name": "group"
        },
        {
          "type": "SYMBOL",
          "name": "optext"
        },
        {
          "type": "SYMBOL",
          "name": "command"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "comment": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "TOKEN",
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "%"
            },
            {
              "type": "PATTERN",
              "value": ".*"
            }
          ]
        }
      }
    },
    "escapechar": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "STRING",
          "value": "%"
        },
        {
          "type": "STRING",
          "value": "&"
        },
        {
          "type": "STRING",
          "value": "^"
        },
        {
          "type": "STRING",
          "value": "_"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "STRING",
          "value": "}"
        },
        {
          "type": "STRING",
          "value": "|"
        },
        {
          "type": "STRING",
          "value": "~"
        },
        {
          "type": "STRING",
          "value": "\\"
        }
      ]
    },
    "escaped": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\\"
          },
          {
            "type": "SYMBOL",
            "name": "escapechar"
          }
        ]
      }
    },
    "group": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "{"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_content"
            }
          },
          {
            "type": "STRING",
            "value": "}"
          }
        ]
      }
    },
    "command": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\\"
          },
          {
            "type": "SYMBOL",
            "name": "name"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "PATTERN",
                      "value": "\\s+"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "SYMBOL",
                  "name": "optionblock"
                }
              ]
            }
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "\\s+"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "optionblock": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "["
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "_options"
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "REPEAT",
                        "content": {
                          "type": "SEQ",
                          "members": [
                            {
                              "type": "STRING",
                              "value": ","
                            },
                            {
                              "type": "CHOICE",
                              "members": [
                                {
                                  "type": "PATTERN",
                                  "value": "\\s+"
                                },
                                {
                                  "type": "BLANK"
                                }
                              ]
                            },
                            {
                              "type": "SYMBOL",
                              "name": "_options"
                            }
                          ]
                        }
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "STRING",
            "value": "]"
          }
        ]
      }
    },
    "_options": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "opkey"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "="
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SYMBOL",
                      "name": "opval"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "opkey": {
      "type": "PATTERN",
      "value": "[^=,\\[\\] ]+"
    },
    "opval": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_opcontent"
      }
    },
    "text": {
      "type": "PATTERN",
      "value": "[^\\[\\]$%&^_{}|~\\\\]+"
    },
    "optext": {
      "type": "PATTERN",
      "value": "[^\\\\{}\\[\\]\\s,][^\\\\{}\\[\\],]*"
    },
    "name": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9]+"
    },
    "_newline": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "STRING",
        "value": "\n"
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

